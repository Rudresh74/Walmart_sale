{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Fifa-DF"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://fifastrg2.dfs.core.windows.net/"
		},
		"AzureKeyVaultWalmart_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://fifa-keyV.vault.azure.net/"
		},
		"Http_allFiles_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().BaseURL}"
		},
		"OnPrimToAzure_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "localhost"
		},
		"OnPrimToAzure_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "walmartDB"
		},
		"OnPrimToAzure_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "DBuser"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Fifa-DF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureKeyVaultWalmart_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVaultWalmart_properties_typeProperties_baseUrl')]"
					},
					"Http_allFiles_properties_typeProperties_url": {
						"value": "[parameters('Http_allFiles_properties_typeProperties_url')]"
					},
					"OnPrimToAzure_properties_typeProperties_server": {
						"value": "[parameters('OnPrimToAzure_properties_typeProperties_server')]"
					},
					"OnPrimToAzure_properties_typeProperties_database": {
						"value": "[parameters('OnPrimToAzure_properties_typeProperties_database')]"
					},
					"OnPrimToAzure_properties_typeProperties_username": {
						"value": "[parameters('OnPrimToAzure_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}